{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "require": {
        "php": "^7.3",
        "facade/ignition": "^2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hisorange/browser-detect": "^4.3",
        "inertiajs/inertia-laravel": "^0.3.2",
        "kg-bot/laravel-localization-to-vue": "^1.6",
        "laravel/fortify": "^1.6",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.3",
        "laravel/telescope": "^4.2",
        "laravel/tinker": "^2.0",
        "league/flysystem-sftp": "^1.0",
        "php-twinfield/twinfield": "^2.9",
        "sammyjo20/lasso": "^1.2",
        "spatie/browsershot": "^3.42",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-medialibrary-pro": "^1.12",
        "spatie/laravel-permission": "^3.17",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.8",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6.9",
        "pestphp/pest": "^0.3.9",
        "pestphp/pest-plugin-laravel": "^0.3.0",
        "phpunit/phpunit": "9.4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
